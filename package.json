{
  "name": "ziptron-backend",
  "version": "1.0.0",
  "description": "Backend for Ziptron EV charging application",
  "type": "commonjs",
  "main": "src/server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "tsx src/server.ts",
    "gen:pg": "prisma generate --schema=prisma/postgres/schema.prisma",
    "gen:mg": "prisma generate --schema=prisma/mongodb/schema.prisma",
    "gen:all": "npm run gen:pg && npm run gen:mg",
    "db:migrate": "prisma migrate dev --schema=prisma/postgres/schema.prisma",
    "db:push:mg": "prisma db push --schema=prisma/mongodb/schema.prisma"
  },
  "keywords": [
    "ev",
    "charging",
    "ziptron"
  ],
  "license": "ISC",
  "dependencies": {
    "@clerk/express": "^1.7.63",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "6.19",
    "@upstash/redis": "^1.36.1",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "mongodb": "^7.0.0",
    "morgan": "^1.10.1",
    "nodemailer": "^7.0.12",
    "pg": "^8.17.1",
    "svix": "^1.84.1",
    "winston": "^3.19.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.30",
    "@types/nodemailer": "^7.0.5",
    "@types/pg": "^8.16.0",
    "prisma": "6.19",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
